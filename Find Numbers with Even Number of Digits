int findNumbers(int* nums, int numsSize)
{
    int i,n,max=0;
    for(i=0;i<numsSize;i++)
    {  
        int count=0;
        n=nums[i];
        while(n>0)
        {
            n=n/10;
            count++;
        }   
        if(count%2==0)
        {
            max++;
        }
    }
    return max;
}
